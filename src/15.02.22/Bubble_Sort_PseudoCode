NUMBERS = [10,29,34,12,56,72,98,1,22,45,0]

output "Before sorting"
loop N from 0 to 10
   output NUMBERS[N]
end loop

output "Sorted array:"

loop I from 0 to 9
    loop CUR from 0 to 9
        if NUMBERS[CUR] > NUMBERS[CUR+1] then
          TEMP = NUMBERS[CUR]
          NUMBERS[CUR] = NUMBERS[CUR+1]
          NUMBERS[CUR+1] = TEMP
        end if
    end loop
end loop

loop J from 0 to 10
   output NUMBERS[J]
end loop
